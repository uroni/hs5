{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "objects": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "integer",
                        "enum": [0, 1, 2]
                    },
                    "name": {
                        "type": "string"
                    },
                    "size": {
                        "type": "integer"
                    },
                    "lastModified": {
                        "type": "integer"
                    },
                    "created": {
                        "type": "integer"
                    }
                },
                "required": ["name", "type", "size", "lastModified", "created"]
            }
        },
        "nextMarker": {
            "type": "string"
        },
        "isTruncated": {
            "type": "boolean"
        }
    },
    "required": ["objects", "nextMarker", "isTruncated"]
}

