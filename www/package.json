{
  "name": "hs5-webinterface",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "openapi-ts": "openapi-ts --client legacy/fetch -i ../src/api.yml -o ./src/api/",
    "errapigen": "quicktype -s schema ../src/schemas/HapiError.json -o src/errorapi/HapiError.ts",
    "format": "pnpm exec prettier . --write"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.73.0",
    "@fluentui/react-experiments": "^8.14.197",
    "@fluentui/react-icons": "^2.0.239",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.61.5",
    "esbuild": "^0.27.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.9.0",
    "valtio": "^1.13.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@hey-api/openapi-ts": "^0.64.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.2.5",
    "quicktype": "^23.2.6",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.24.1",
    "vite": "^5.4.14"
  }
}
